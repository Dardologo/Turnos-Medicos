@model Especialidad;

<h3>¿ Esta seguro de eliminar el siguiente registro?</h3>
<div>
    <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><b>  @Html.DisplayFor(model => model.Descripcion)</b> </span>
         
        </div>
        <div class="card-action green darken-4">
           <form asp-action="Delete">
                    <input type="hidden" asp-for="IdEspecialidad" />
                    <a asp-action="Delete" asp-route-id="@Model.IdEspecialidad"><i class="material-icons left ">delete</i> </a>
                    <a asp-action="Index" class="right">Volver</a>
                </form>
        </div>
      </div>
    </div>
  </div>
    
</div>
