@model IEnumerable<Especialidad> 

 <h1>Especialidades</h1>
 <p>
    <a asp-action="Create" class="btn-floating btn-large grey darken-1"> <i class="material-icons">medical_services</i></a>
 </p>
        <table class="responsive-table highlight "z-depth-3" brown darken-2>
            <thdead class="blue darken-1 white-text">
                <tr>
                    <th>
                       @Html.DisplayNameFor(model=> model.Descripcion)
                    </th>
                </tr>
            </thdead>
            <tbody>
                
                    @foreach (var item in Model)
                {
                    <tr>
                        
                        <td>
                            @Html.DisplayFor(modelItem=> item.Descripcion)
                          
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id= "@item.IdEspecialidad"><i class="material-icons">edit</i></a>
                        </td>
                        <td>
                            <a asp-action="Delete" asp-route-id= "@item.IdEspecialidad"><i class="material-icons red-text">delete</i></a>
                        </td>
                    </tr>
                   
                }
                

              </tbody>
                    

        </table>
        <br/>
          <br/>
            <br/>
